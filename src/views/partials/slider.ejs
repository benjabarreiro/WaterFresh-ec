<div class="container">

    <div class="row">

      <div class="col s12">

        <h1 class="center-align titulo">Bienvenido</h1>

        <div class="carousel center-align">

        <% for(let i=0; i<products.length; i++) { %> 

            <div class="carousel-item">
              <h2 class="subtitulo"><%= products[i].title %> </h2>
              <img src="img-slider/<%= products[i].img_slider %> "  alt="">
            </div>

        <% } %>

        </div>

      </div>

    </div>

  </div>